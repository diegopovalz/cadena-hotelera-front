{
  "name": "cliente",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "0.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.3",
    "react-router-dom": "6.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "1.0.7",
    "vite": "2.8.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.1.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "linebreak-style": 0,
      "comma-dangle": [
        "error",
        {
          "arrays": "never",
          "objects": "always",
          "imports": "never",
          "exports": "never",
          "functions": "never"
        }
      ],
      "react/prop-types": 0,
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-one-expression-per-line": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off"
    },
    "ignorePatterns": [
      "vite.config.js"
    ]
  }
}
